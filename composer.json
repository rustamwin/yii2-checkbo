{
    "name": "rustamwin/yii2-checkbo",
    "description": "CheckBo widget for Yii2.",
    "type": "yii2-extension",
    "version": "1.0",
    "keywords": [
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "checkBo"
    ],
    "homepage": "https://github.com/rustamwin/yii2-checkbo",
    "license": "MIT",
    "authors": [
        {
            "name": "Rustam Mamadaminov",
            "email": "rmamdaminov@gmail.com",
            "homepage": "http://rustamwin.github.io",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/rustamwin/yii2-checkbo/issues",
        "source": "https://github.com/rustamwin/yii2-checkbo"
    },
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "bower-asset/checkbo": "v0.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "rustamwin\\checkbo\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
